Given an Integer N, write a program to reverse it.


import java.util.*;
import java.lang.*;
import java.io.*;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int i=0; i<T; i++){
		    int N = sc.nextInt();
		    int reverse = 0;
		    while(N!=0){
		        int lastDigit = N%10;
		        reverse = (reverse*10)+lastDigit;
		        N = N/10;
		    }
		    System.out.println(reverse);
		} 
	}
}
